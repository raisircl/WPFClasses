<Window x:Class="WpfPanelsApp.WindowAppWrapPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfPanelsApp"
        mc:Ignorable="d"
        Title="WindowAppWrapPanel" Height="450" Width="250">
    <WrapPanel >
        <Label x:Name="label" Content="Label"/>
        <TextBox x:Name="textBox" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        <Button x:Name="button2" Content="Button"/>
        <Label x:Name="label2" Content="Label"/>
        <TextBox x:Name="textBox2" TextWrapping="Wrap" Text="TextBox" Width="120"/>
        <Button x:Name="button3" Content="Button"/>

    </WrapPanel>
</Window>
